    }
   ],
   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "%matplotlib inline\n",
    "\n",
    "# Training loss values from step 10 to 900\n",
    "steps = list(range(10, 910, 10))  # Steps from 10 to 900 in increments of 10\n",
    "\n",
    "# Loss values\n",
